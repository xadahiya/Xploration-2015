                {
                    "@type" : "http://www.w3.org/2002/07/owl#Restriction",
                    "owl:onProperty" : "http://ontology.projectchronos.eu/subsystems/hasWireOutWith",
                    "owl:hasValue" : [
                      { "@id": "http://ontology.projectchronos.eu/subsystems/Spacecraft_Communication" },
                      { "@id": "http://ontology.projectchronos.eu/subsystems/Spacecraft_CDH" }
                    ]
                },
                {
                    "@type" : "http://www.w3.org/2002/07/owl#Restriction",
                    "owl:onProperty" : "http://ontology.projectchronos.eu/subsystems/hasWireInWith",
                    "owl:hasValue" : "http://ontology.projectchronos.eu/subsystems/Spacecraft_PrimaryPower"
                },
                {
                    "@type" : "http://www.w3.org/2002/07/owl#Restriction",
                    "owl:onProperty" : "http://ontology.projectchronos.eu/subsystems/hasMinAmpere",
                    "owl:minCardinality" : 1
                },
                              {
                    "@type" : "http://www.w3.org/2002/07/owl#Restriction",
                    "owl:onProperty" : "http://ontology.projectchronos.eu/subsystems/hasMaxAmpere",
                    "owl:minCardinality" : 1
                },
                {
                    "@type" : "http://www.w3.org/2002/07/owl#Restriction",
                    "owl:onProperty" : "http://ontology.projectchronos.eu/subsystems/hasVoltage",
                    "owl:minCardinality" : 1
                }
            ],
