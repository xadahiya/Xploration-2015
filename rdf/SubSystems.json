    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "owl": "http://www.w3.org/2002/07/owl#",
    "dbpedia": "http://dbpedia.org/ontology/",
    "xml": "http://www.w3.org/2001/XMLSchema#",
    "defines": {
            "@reverse": "rdfs:isDefinedBy"
    }
  },
  "@id": "",
  "@type": "http://www.w3.org/2002/07/owl#Ontology",
  "rdfs:label": "Subsystems that run tasks that are needed for the spacecraft to be working and fulfilling the objectives of the mission. Words separated by _ has to be read as separated concepts, camelCase in they are the same concepts (see umbel.org for this norm)",
  "rdfs:comment": "These ontology contains classes of different subsystems present in a spacecraft.",
  "defines": [
         {
           "@id" : "http://ontology.projectchronos.eu/subsystems/manufacturer",
           "@type" : [
                 "http://www.w3.org/2002/07/owl#ObjectProperty"
           ],
           "rdfs:label": "manufacturer"
         },
         {
            "@id" : "http://ontology.projectchronos.eu/subsystems/function",
            "@type" : [
                "http://www.w3.org/2002/07/owl#AsymmetricProperty",
                "http://www.w3.org/2002/07/owl#IrreflexiveProperty",
                "http://www.w3.org/2002/07/owl#ObjectProperty"
            ],
            "rdf:domain" : { "@id": "http://ontology.projectchronos.eu/spacecraft/Subsystem_Spacecraft" },
            "rdfs:comment" : "The function and the objective what the device performs or make possible",
            "rdf:label" : "what the device is used for",
            "rdfs:label" : "function",
            "skos:altLabel" : "is used for"
        },
        {
            "@id" : "http://ontology.projectchronos.eu/subsystems/unitOfMeasure",
            "@type" : [
                "http://www.w3.org/2002/07/owl#AsymmetricProperty",
                "http://www.w3.org/2002/07/owl#IrreflexiveProperty",
                "http://www.w3.org/2002/07/owl#ObjectProperty"
            ],
            "owl:sameAs": [
                { "@id": "http://umbel.org/umbel/rc/UnitOfMeasure"}, 
                { "@id": "http://dbpedia.org/resource/Units_of_measurement" }
            ],
            "rdf:domain" : { "@id": "http://ontology.projectchronos.eu/spacecraft/Subsystem_Spacecraft" },
            "rdf:range" : { "@id": "http://www.w3.org/1999/02/22-rdf-syntax-ns#PlainLiteral" },
            "rdfs:comment" : "Property to set the unit of measure used by a physical value",
            "rdfs:label" : "unitOfMeasure"
        },
        {
            "@id": "http://ontology.projectchronos.eu/subsystems/subSystemType",
            "rdf:domain" : { "@id": "http://ontology.projectchronos.eu/spacecraft/Subsystem_Spacecraft" },
            "skos:prefLabel": "A property that references the subsystem to the kind of the devices it holds.",
            "rdfs:comment": "Every subsystem contains an homogeneous group of devices.",
            "rdfs:label": "subSystemType",
            "@type" : "http://www.w3.org/2002/07/owl#ObjectProperty"
        },
        {
            "@id": "http://ontology.projectchronos.eu/subsystems/isStandard",
            "rdf:domain" : { "@id": "http://ontology.projectchronos.eu/spacecraft/Subsystem_Spacecraft" },
            "skos:prefLabel": "A property that references the standard platform for which the subsystem has been designed.",
            "rdfs:comment": "There are many different standards that imply quantitative and qualitative differences",
            "rdfs:label": "isStandard",
            "@type" : "http://www.w3.org/2002/07/owl#ObjectProperty"

        },
        {
            "@id": "http://ontology.projectchronos.eu/subsystems/hasVolume",
            "rdf:domain" : { "@id": "http://ontology.projectchronos.eu/spacecraft/Subsystem_Spacecraft" },
            "rdf:range" : { "@id": "http://www.w3.org/2001/XMLSchema#float" },
            "skos:exactMatch": [
