                { "@id": "http://dbpedia.org/resource/Volume" }
            ],
            "rdfs:label": "hasVolume",
            "@type" : "http://www.w3.org/2002/07/owl#ObjectProperty"
        },
        {
            "@id": "http://ontology.projectchronos.eu/subsystems/hasMinAmpere",
            "rdf:domain" : { "@id": "http://ontology.projectchronos.eu/spacecraft/Subsystem_Spacecraft" },
            "rdf:range" : { "@id": "http://www.w3.org/2001/XMLSchema#float" },
            "skos:exactMatch": [
                { "@id": "http://umbel.org/umbel/rc/Ampere" }, 
                { "@id": "http://dbpedia.org/resource/Ampere" }
            ],
            "rdfs:label": "hasMinAmpere",
            "@type" : "http://www.w3.org/2002/07/owl#ObjectProperty"
        },
        {
            "@id": "http://ontology.projectchronos.eu/subsystems/hasMaxAmpere",
            "rdf:domain" : { "@id": "http://ontology.projectchronos.eu/spacecraft/Subsystem_Spacecraft" },
            "rdf:range" : { "@id": "http://www.w3.org/2001/XMLSchema#float" },
            "skos:exactMatch": [
                { "@id": "http://umbel.org/umbel/rc/Ampere" }, 
                { "@id": "http://dbpedia.org/resource/Ampere" }
            ],
            "rdfs:label": "hasMaxAmpere",
            "@type" : "http://www.w3.org/2002/07/owl#ObjectProperty"
        },
        {
            "@id": "http://ontology.projectchronos.eu/subsystems/hasMass",
            "rdf:domain" : { "@id": "http://ontology.projectchronos.eu/spacecraft/Subsystem_Spacecraft" },
            "rdf:range" : { "@id": "http://www.w3.org/2001/XMLSchema#float" },
            "skos:exactMatch": [
                { "@id": "http://umbel.org/umbel/rc/Mass" }, 
                { "@id": "http://dbpedia.org/resource/Mass" }, 
                { "@id": "http://schema.org/Mass" }
            ],
            "rdfs:label": "hasMass",
            "@type" : "http://www.w3.org/2002/07/owl#ObjectProperty"
        },
        {
            "@id": "http://ontology.projectchronos.eu/subsystems/hasPower",
            "rdf:domain" : { "@id": "http://ontology.projectchronos.eu/spacecraft/Subsystem_Spacecraft" },
            "rdf:range" : { "@id": "http://www.w3.org/2001/XMLSchema#float" },
            "skos:exactMatch": [
                { "@id": "http://umbel.org/umbel/rc/Power" }, 
                { "@id": "http://dbpedia.org/resource/Power_(physics)"}
            ],
            "rdfs:label": "hasPower",
            "@type" : "http://www.w3.org/2002/07/owl#ObjectProperty"
        },
        {
            "@id": "http://ontology.projectchronos.eu/subsystems/hasSpecificImpulse",
            "rdf:domain" : { "@id": "http://ontology.projectchronos.eu/spacecraft/Subsystem_Spacecraft" },
            "rdf:range" : { "@id": "http://www.w3.org/2001/XMLSchema#float" },
            "skos:exactMatch": { "@id": "http://dbpedia.org/resource/Specific_impulse" },
            "rdfs:label": "hasSpecificImpulse",
            "@type" : "http://www.w3.org/2002/07/owl#ObjectProperty"
        },
        {
            "@id": "http://ontology.projectchronos.eu/subsystems/hasMonetaryValue",
            "rdf:domain" : { "@id": "http://ontology.projectchronos.eu/spacecraft/Subsystem_Spacecraft" },
            "rdf:range" : { "@id": "http://www.w3.org/2001/XMLSchema#float" },
            "skos:exactMatch": [
                { "@id": "http://dbpedia.org/resource/Price" }, 
                { "@id": "http://umbel.org/umbel/rc/MonetaryValue" }, 
                { "@id": "http://schema.org/PriceSpecification" }
            ],
            "rdfs:label": "hasMonetaryValue",
            "rdfs:comment": "Amount of money it can be bought for, or an esteem of value.",
            "@type" : "http://www.w3.org/2002/07/owl#ObjectProperty"
        },
        {
            "@id": "http://ontology.projectchronos.eu/subsystems/hasWireInWith",
            "rdf:domain" : { "@id": "http://ontology.projectchronos.eu/spacecraft/Subsystem_Spacecraft" },
            "rdf:range" : { "@id": "http://ontology.projectchronos.eu/spacecraft/Subsystem_Spacecraft" },
            "rdfs:label": "hasWireInWith",
            "rdfs:comment": "This device receive input from another device",
            "@type" : "http://www.w3.org/2002/07/owl#ObjectProperty"
        },
        {
            "@id": "http://ontology.projectchronos.eu/subsystems/hasWireOutWith",
            "rdf:domain" : { "@id": "http://ontology.projectchronos.eu/spacecraft/Subsystem_Spacecraft" },
            "rdf:range" : { "@id": "http://ontology.projectchronos.eu/spacecraft/Subsystem_Spacecraft" },
            "rdfs:label": "hasWireOutWith",
            "rdfs:comment": "This device send output to another device",
            "@type" : "http://www.w3.org/2002/07/owl#ObjectProperty"
        },
        {
            "@id": "http://ontology.projectchronos.eu/subsystems/typeOfPropellant",
            "rdf:domain" : { "@id": "http://ontology.projectchronos.eu/spacecraft/Spacecraft_Propulsion" },
            "rdfs:label": "typeOfPropellant",
            "rdfs:comment": "Type of fueling used by a spacecraft rocket engine",
            "@type" : "http://www.w3.org/2002/07/owl#ObjectProperty"
        },
        {
            "@id": "http://ontology.projectchronos.eu/subsystems/Spacecraft_Detector",
            "@type" : "http://www.w3.org/2002/07/owl#Class",
            "rdfs:subClassOf" : [
                { "@id": "http://umbel.org/umbel/rc/Sensor_Device" },
                { "@id": "http://ontology.projectchronos.eu/spacecraft/SubSystems_Spacecraft" },
                {
                    "@type" : "http://www.w3.org/2002/07/owl#Restriction",
                    "owl:onProperty" : "http://ontology.projectchronos.eu/subsytems/objective",
                    "owl:minCardinality" : 1
                },
                {
                    "@type" : "http://www.w3.org/2002/07/owl#Restriction",
                    "owl:onProperty" : "http://ontology.projectchronos.eu/subsytems/isComponentOf",
                    "owl:hasValue" : "http://ontology.projectchronos.eu/spacecraft/Payload_Spacecraft"
